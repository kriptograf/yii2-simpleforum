{
    "name": "ivan/yii2-simpleforum",
    "description": "This extension will add a simple forum with minimal efforts.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","simpleforum","simple forum"],
    "license": "MIT",
    "support": {
        "source": "https://github.com/catalinivan15/yii2-simpleforum"
    },
    "authors": [
        {
            "name": "Catalin Ivan",
            "email": "catalin.ivan15@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii2": "*",
        "dektrium/yii2-user": "*",
        "nemmo/yii2-attachments": "*",
        "kartik-v/yii2-markdown": "*"
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/catalinivan15/yii2-simpleforum"
        }
    ],
    "autoload": {
        "psr-4": {
            "ivan\\simpleforum\\": ""
        }
    }
}
